<section2 class="get">
    <h3>Animal Feeds Purchased</h3>
    <div>
        {{# each amounts }}
            <article class="amount">
                <b>Quantity:</b> {{ this.qty }}
                <b>Price:</b> {{ this.price }}
                <b>Supplier:</b> {{ this.supplier }}
                <b>Type:</b> {{ this.type }}
                <b>Date:</b> {{ this.date1 }}
                <b>ID:</b> {{ this._id }}
            </article>
        {{/each}}
    </div>
</section2>

<section2 class="get">
    <h3>Vet Receipts</h3>
    <div>
        {{# each units }}
            <article class="unit">
                <b>Tag:</b> {{ this.tag1 }}
                <b>Reason:</b> {{ this.reason }}
                <b>Vet Name:</b> {{ this.vetname }}
                <b>Cost:</b> {{ this.cost }}
                <b>Date:</b> {{ this.date }}
                <b>ID:</b> {{ this._id }}
            </article>
        {{/each}}
    </div>
</section2>
